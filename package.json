{
  "name": "louis-amy-proposal-platform",
  "version": "1.0.0",
  "description": "Interactive proposal platform for Louis Amy AE Studio implementing Chris Do's value-based pricing philosophy",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc -p tsconfig.app.json --noEmit",
    "test:type-check": "tsc -p tsconfig.test.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:setup": "tsx scripts/setup-database.ts",
    "db:seed": "tsx scripts/seed-data.ts",
    "db:migrate": "supabase db push",
    "validate:formulas": "tsx scripts/validate-formulas.ts",
    "calibrate": "tsx scripts/calibrate-from-excel.ts",
    "calibrate:validate": "tsx scripts/calibrate-from-excel.ts validate",
    "calibration:excel": "tsx scripts/switch-calibration-mode.ts excel",
    "calibration:ssot": "tsx scripts/switch-calibration-mode.ts ssot",
    "calibration:show": "tsx scripts/switch-calibration-mode.ts show",
    "verify:alignment": "tsx scripts/verify-alignment.ts",
    "verify:csv": "tsx scripts/verify-csv-alignment.ts",
    "db:setup:schema": "tsx scripts/setup-db.ts",
    "db:import:csv": "tsx scripts/import-csv-to-supabase.ts",
    "db:create:proposals": "tsx scripts/create-proposals-table.ts",
    "db:add:notes": "tsx scripts/add-notes-column.ts",
    "db:schema:print": "type supabase/schema.pr_construction_cost_index_2025.sql",
    "db:import:index": "tsx scripts/import-construction-cost-index.ts",
    "deploy": "vercel --prod",
    "analyze": "cross-env ANALYZE=true next build"
  },
